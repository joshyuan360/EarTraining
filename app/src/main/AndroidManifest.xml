<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.joshuayuan.eartraining">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.ChordsActivity" />
        <activity android:name=".activity.CadencesActivity" />
        <activity android:name=".activity.IntervalsActivity" />
        <activity android:name=".activity.HighScores" />
        <activity android:name=".activity.PreferencesActivity" />
        <activity android:name=".activity.ChordProgressionActivity"/>
    </application>

</manifest>